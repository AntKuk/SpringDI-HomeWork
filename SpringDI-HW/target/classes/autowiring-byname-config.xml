<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="anton" class="com.netcracker.autowiring.Musician" scope="prototype" autowire="byName">
        <property name="song" value="Happy New Year"/>
    </bean>

    <bean id="instrument" class="com.netcracker.autowiring.Guitar" scope="prototype" />
    <bean id="saxophone" class="com.netcracker.autowiring.Saxophone" scope="prototype"/>

</beans>